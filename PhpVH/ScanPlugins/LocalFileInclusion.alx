#'Std';

slashes = [ '\\', '/' ];
dotLens = [ 2, 6 ];
travSeqLens = [ 8, 32 ];
exts = [ 'jpg' ];
probe = 'lfi_test.txt';

travSeqs = dotLens
    .selectMany(@(x)slashes
        .select(@(y)'.'*x+y)
        .selectMany(@(x)travSeqLens.select(@(y)x*y+probe)));

if (exts? && exts.any()) 
    travSeqs = travSeqs
        .selectMany(@(x)exts.select(@(y)x+chr(0)+'.'+y))
        .concat(travSeqs);

ret travSeqs.distinct();